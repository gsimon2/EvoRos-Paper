@article{Ruina2005,
	Abstract = {Terrestrial legged locomotion requires repeated support forces to redirect the body's vertical velocity component from down to up. We assume that the redirection is accomplished by impulsive leg forces that cause small-angle glancing collisions of a point-mass model of the animal. We estimate the energetic costs of these collisions by assuming a metabolic cost proportional to positive muscle work involved in generating the impulses. The cost of bipedal running estimated from this collisional model becomes less than that of walking at a Froude number (v2/gl) of about 0.7. Two strategies to reduce locomotion costs associated with the motion redirection are: (1) having legs simulate purely elastic springs, as is observed in human running; and (2) sequencing the leg forces during the redirection phase; examples of this sequencing are the ba-da-dump pattern of a horse gallop and having push-off followed by heel-strike in human walking.},
	Author = {Ruina, Andy and Bertram, John E. A. and Srinivasan, Manoj},
	Date-Added = {2015-01-06 14:16:24 +0000},
	Date-Modified = {2015-02-01 16:53:22 +0000},
